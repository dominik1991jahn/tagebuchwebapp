<?xml version="1.0" encoding="UTF-8"?>
<RequestHandlerMapping>
	<Request method="GET" url="/Class" handler="Tunnel.GetClassList" />
	
	<Request method="GET" url="/Subject/([a-zA-Z0-9]+)" handler="Tunnel.GetSubjectsForClass">
		<Field>Class</Field>
	</Request>
	
	<Request method="GET" url="/Teacher/([A-Za-z0-9]+)" handler="Tunnel.GetTeacherList">
		<Field>Class</Field>
	</Request>
	
	<Request method="GET" url="/Teacher" handler="Tunnel.GetTeacherListForTeacher" />
	
	<Request method="GET" url="/Schedule/([A-Za-z0-9]+)-(([0-9]{4})-([0-9]{2})-([0-9]{2}))-(([0-9]{4})-([0-9]{2})-([0-9]{2}))">
		<Field>Class</Field>
		<Field>From</Field>
		<Field>To</Field>
	</Request>
</RequestHandlerMapping>